# ========================================================================
# This config file can be used to setup the default descriptions for nodes
# WARNING: Adding this file will degrade performance
#   if node descriptions are not set
# ========================================================================
# Default if no description is set.  Use DATE for date and NODE for node
default:
  description: Created on DATE.
  alias: NODE
    
# Node specific names
admin:
  description: Crowbar Server
  alias: Admin
  group: Core

d00-0c-29-ef-57-88:
  description: Controller
  alias: Node 1

ASSET_TAG_001:
  description: Dell Server 2
  alias: Node 2